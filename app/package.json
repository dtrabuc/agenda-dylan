{
  "name": "agenda-backend",
  "version": "1.0.0",
  "description": "Backend API pour application d'agenda personnel",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "prisma generate --schema=prisma/schema.postgresql.prisma",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:generate:postgresql": "prisma generate --schema=prisma/schema.postgresql.prisma",
    "db:push:postgresql": "prisma db push --schema=prisma/schema.postgresql.prisma",
    "db:migrate:postgresql": "prisma migrate deploy --schema=prisma/schema.postgresql.prisma"
  },
  "keywords": ["agenda", "api", "express", "prisma", "node"],
  "author": "Dylan",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "prisma": "^6.16.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
